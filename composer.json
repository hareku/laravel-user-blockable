{
    "name": "hareku/laravel-user-blockable",
    "description": "Laravel 5 user block system",
    "keywords": ["laravel", "block"],
    "license": "MIT",
    "authors": [
        {
            "name": "hareku",
            "email": "hareku908@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/database": ">=5.4",
        "illuminate/support": ">=5.4",
        "nesbot/carbon": "~1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "Hareku\\LaravelBlockable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hareku\\LaravelBlockable\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
